{
    "name": "akeneo/template-generator",
    "type": "project",
    "require": {
        "php": "8.1.*",
        "openspout/openspout": "^4.15",
        "symfony/console": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Akeneo\\TemplateGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akeneo\\Test\\TemplateGenerator\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.23"
    }
}
